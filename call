public class IntentExample extends AppCompatActivity {
 Button btnExplicit, btnCall, btnDialer, btnContact, btnSMS, 
btnContactDetails, btnCallLog, btnEmail, btnMap, btnURL;
 @Override
 protected void onCreate(Bundle savedInstanceState) {
 super.onCreate(savedInstanceState);
 EdgeToEdge.enable(this);
 setContentView(R.layout.activity_intent_example);
 ViewCompat.setOnApplyWindowInsetsListener(findViewById(R.id.main), 
(v, insets) -> {
 Insets systemBars = 
insets.getInsets(WindowInsetsCompat.Type.systemBars());
 v.setPadding(systemBars.left, systemBars.top, systemBars.right, 
systemBars.bottom);
 return insets;
 });
 controlInitialization();
 buttonClicks();
 }
 protected void controlInitialization() {
 btnExplicit = findViewById(R.id.btnExplicit);
 btnCall = findViewById(R.id.btnCall);
 btnDialer = findViewById(R.id.btnDialer);
 btnMap = findViewById(R.id.btnMap);
 btnURL = findViewById(R.id.btnURL);
 btnContact = findViewById(R.id.btnContact);
 btnSMS = findViewById(R.id.btnSMS);
 btnContactDetails = findViewById(R.id.btnContactDetails);
 btnCallLog = findViewById(R.id.btnCallLog);
 btnEmail = findViewById(R.id.btnEmail);
 }
 protected void buttonClicks() {
 // Explicit Intent
 btnExplicit.setOnClickListener(new View.OnClickListener() {
 @Override
 public void onClick(View v) {
 Intent objExplicit = new Intent(getApplicationContext(), 
MainActivity.class);
 startActivity(objExplicit);
 }
 });
 // Open Map
 btnMap.setOnClickListener(new View.OnClickListener() {
 @Override
 public void onClick(View v) {
 Intent objMap = new Intent(Intent.ACTION_VIEW, 
Uri.parse("geo:21.0678313,73.128108"));
 startActivity(objMap);
 }
 });
 // Open URL
 btnURL.setOnClickListener(new View.OnClickListener() {
@Override
 public void onClick(View v) {
 Intent objURL = new Intent(Intent.ACTION_VIEW, 
Uri.parse("https://www.google.com"));
 startActivity(objURL);
 }
 });
 // Open Dialer
 btnDialer.setOnClickListener(new View.OnClickListener() {
 @Override
 public void onClick(View v) {
 Intent objDialer = new Intent(Intent.ACTION_DIAL, 
Uri.parse("tel:9876543210"));
 startActivity(objDialer);
 }
 });
 //Make Phone Call
 btnCall.setOnClickListener(new View.OnClickListener() {
 @Override
 public void onClick(View v) {
 if (ActivityCompat.checkSelfPermission(IntentExample.this, 
android.Manifest.permission.CALL_PHONE) != 
PackageManager.PERMISSION_GRANTED) {
 ActivityCompat.requestPermissions(IntentExample.this, 
new String[]{android.Manifest.permission.CALL_PHONE}, 0 );
 }
 Intent objCall = new Intent(Intent.ACTION_CALL, 
Uri.parse("tel:9876543210"));
 startActivity(objCall);
 }
 });
 // Open Contact Details
 btnContactDetails.setOnClickListener(new View.OnClickListener() {
 @Override
 public void onClick(View v) {
 Intent objContact = new Intent(Intent.ACTION_VIEW, 
Uri.parse("content://contacts/people/1"));
 startActivity(objContact);
 }
 });
 //Open Contacts
 btnContact.setOnClickListener(new View.OnClickListener() {
 @Override
 public void onClick(View v) {
 Intent objContactDetails = new Intent(Intent.ACTION_VIEW, 
Uri.parse("content://contacts/people/"));
 startActivity(objContactDetails);
 }
 });
 //Show Call Log
 btnCallLog.setOnClickListener(new View.OnClickListener() {
 @Override
 public void onClick(View v) {
 Intent objCallLog = new Intent(Intent.ACTION_VIEW,
Uri.parse("content://call_log/calls/"));
 startActivity(objCallLog);
 }
 });
 //Send SMS
 btnSMS.setOnClickListener(new View.OnClickListener() {
 @Override
 public void onClick(View v) {
 Intent objSMS = new Intent(Intent.ACTION_SENDTO);
 objSMS.putExtra("address", "1236547892");
 objSMS.putExtra("sms_body", "Sample Message");
 objSMS.setData(Uri.parse("smsto:"));
 startActivity(objSMS);
 }
 });
 //Send Email
 btnEmail.setOnClickListener(new View.OnClickListener() {
 @Override
 public void onClick(View v) {
 Intent objEmail = new Intent(Intent.ACTION_SEND);
 objEmail.putExtra(Intent.EXTRA_EMAIL, new 
String[]{"21bmiit137@gmail.com"});
 objEmail.putExtra(Intent.EXTRA_SUBJECT, "Demo Email");
 objEmail.putExtra(Intent.EXTRA_TEXT, "Hello, It is Demo 
Email");
 objEmail.setType("message/rfc822");
 startActivity(objEmail);
 }
 });
 }
}